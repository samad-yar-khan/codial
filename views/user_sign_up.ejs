<h1>Sign Up</h1>

<form action="/users/create" method="POST">
    <input type="text" name="name" required placeholder="Your Name" required>
    <input type="email" name="email" required placeholder="Your Email" required>
    <input type="password" name="password" required placeholder="password" required>
    <input type="password" name="confirmPassword" required placeholder="confirm password" required>
    <input type="submit" value="Sign Up">
</form>